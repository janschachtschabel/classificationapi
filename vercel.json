{
  "functions": {
    "api/index.py": {
      "runtime": "python3.12",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.py"
    }
  ],
  "env": {
    "OPENAI_DEFAULT_MODEL": "gpt-4o-mini",
    "API_HOST": "0.0.0.0",
    "LOG_LEVEL": "INFO",
    "LOG_FORMAT": "json",
    "PYTHONPATH": "."
  }
}
